<head>
<link href="/cse-gui/css/bootstrap.min.css" rel="stylesheet" type="text/css">
<link href="/cse-gui/css/DT_bootstrap.css" rel="stylesheet" type="text/css">
<script language="JavaScript" type="text/javascript" src="/cse-gui/js/jquery-1.7.2.min.js"></script>
<script language="JavaScript" type="text/javascript" src="/cse-gui/js/jquery.dataTables.js"></script>
<script language="JavaScript" type="text/javascript" src="/cse-gui/js/paging.js"></script>
</head>
<body>
<div class="container">
	<h1>Experiments</h1>
	<div class="container" >
	<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="example">
		<thead>
			<tr>
				<th>Date</th>
				<th>Name</th>
        <th>Author</th>
        <th>Size</th>
				<th>Id</th>
			</tr>
		</thead>
		<tbody id="experiment-tbody">
	{{#rows}}
<tr>
	<td>{{date}}</td>
	<td>{{name}}</td>
	<td>{{author}}</td>
  <td>{{size}}</td>
	<td><a href="experiment.html?id={{id}}">{{id}}</a></td>
</tr>
{{/rows}}
		</tbody>
	</table>
</div>
</body>
<!-- /container -->
